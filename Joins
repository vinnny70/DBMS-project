INNER JOIN
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE
INNER JOIN CHILDTABLE
ON PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
LEFT JOIN
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE
LEFT JOIN CHILDTABLE
ON PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
RIGHT JOIN
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE
RIGHT JOIN CHILDTABLE
ON PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
FULL JOIN
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE
FULL JOIN CHILDTABLE
ON PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
CARTESIAN
IMPLICIT CROSS
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE
CROSS JOIN CHILDTABLE
WHERE PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
EXPLICIT CROSS
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE,CHILDTABLE
WHERE PARENTTABLE.PARENTID = CHILDTABLE.PARENTID;
SELF JOIN
SELECT PARENTTABLE.PARENTID,PARENTNAME,CHILDTABLE.PARENTID
FROM PARENTTABLE,CHILDTABLE
WHERE PARENTTABLE.PARENTID != CHILDTABLE.PARENTID;
