UNION
SELECT PARENTID,PARENTNAME FROM PARENTTABLE
UNION
SELECT PARENT_ID,DETAIL FROM CHILD_TABLE;
UNION ALL
SELECT PARENTID,PARENTNAME FROM PARENTTABLE
UNION ALL
SELECT PARENT_ID,DETAIL FROM CHILD_TABLE;
MINUS
SELECT PARENTID,PARENTNAME FROM PARENTTABLE
MINUS
SELECT PARENT_ID,DETAIL FROM CHILD_TABLE;
INTERSECT
SELECT PARENTID,PARENTNAME FROM PARENTTABLE
INTERSECT
SELECT PARENT_ID,DETAIL FROM CHILD_TABLE;
