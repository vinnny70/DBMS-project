INSERT
INSERT INTO EMPLOYEES(EMPID,FIRSTNAME,LASTNAME,SALARY)
VALUES(7,'Esha','Malhotra',6600);
VIEW
SELECT * FROM EMPLOYEES;
FILTERING DATA
SELECT *FROM EMPLOYEES WHERE SALARY >6700;
DELETING
DELETE FROM EMPLOYEES
WHERE EMPID = 4;
UPDATIND
UPDATE EMPLOYEES
SET EMPID = 5
WHERE FIRSTNAME = 'Esha';
BETWEEN
SELECT * FROM EMPLOYEES
WHERE SALARY BETWEEN 6700 AND 95000;
ALTER TABLE
ALTER TABLE EMPLOYEES ADD AGE INT;
